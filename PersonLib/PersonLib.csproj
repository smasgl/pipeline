<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>Smasgl</RootNamespace>
    <AssemblyName>Smasgl.Model.$(MSBuildProjectName)</AssemblyName>

    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>

    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>GPL-3.0-only</PackageLicenseExpression>

    <Title>$(MSBuildProjectName)</Title>
    <Authors>Smasgl</Authors>
    <Company>Smasgl</Company>
    <Copyright>(c) 2021 Smasgl</Copyright>

    <PackageProjectUrl>https://github.com/smasgl/pipeline</PackageProjectUrl>
    <PackageTags>person, model, smasgl</PackageTags>
    <Version>1.0.0</Version>
    <Description>Model for Person containing first and lastname</Description>

    <PackageId>Smasgl.Model.PersonLib</PackageId>
    
    <RepositoryUrl>https://github.com/smasgl/pipeline.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

</Project>
